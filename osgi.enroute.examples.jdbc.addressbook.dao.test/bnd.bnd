#
# OSGI ENROUTE EXAMPLES JDBC ADDRESSBOOK DAO TEST OSGi TEST BUNDLE 
#

Bundle-Version:1.0.0.${tstamp}
Bundle-Name: Addressbook DAO Test
Bundle-Description: This bundle is an OSGi test bundle that tests the enRoute JDBC Example DAO \
	API.


Private-Package: \
	osgi.enroute.examples.jdbc.addressbook.dao.test,\
	osgi.enroute.examples.jdbc.addressbook.dao

Test-Cases: ${test-cases}

-runtrace: false

-includeresource: \
	{readme.md} ,\
	{/=resources}

-buildpath: \
	osgi.enroute.base.api;version=1.0,\
	osgi.enroute.junit.wrapper;version=4.12,\
	osgi.enroute.examples.jdbc.addressbook.dao.provider;version=latest,\
	org.osgi.service.jdbc,\
	org.osgi.service.jpa,\
	tx-control-provider-jpa-local,\
	tx-control-service-local

-runrequires: \
	osgi.identity;filter:='(osgi.identity=osgi.enroute.examples.jdbc.addressbook.dao.test)',\
	osgi.identity;filter:='(osgi.identity=osgi.enroute.examples.jdbc.addressbook.dao.provider)',\
	osgi.identity;filter:='(&(osgi.identity=tx-control-service-local)(version>=0.0.2))',\
	osgi.identity;filter:='(&(osgi.identity=tx-control-provider-jpa-local)(version>=0.0.2))',\
	osgi.identity;filter:='(&(osgi.identity=org.apache.felix.configadmin)(version>=1.8.6))',\
	osgi.identity;filter:='(osgi.identity=org.hibernate.osgi)',\
	osgi.identity;filter:='(osgi.identity=org.h2)',\
	osgi.identity;filter:='(&(osgi.identity=org.apache.aries.jpa.container)(version>=2.5.0))'
-runbundles: \
	javassist;version='[3.18.1,3.18.2)',\
	org.apache.aries.jpa.container;version='[2.5.0,2.5.1)',\
	org.apache.felix.configadmin;version='[1.8.6,1.8.7)',\
	org.apache.felix.log;version='[1.0.1,1.0.2)',\
	org.apache.felix.scr;version='[2.0.0,2.0.1)',\
	org.apache.servicemix.bundles.antlr;version='[2.7.7,2.7.8)',\
	org.apache.servicemix.bundles.dom4j;version='[1.6.1,1.6.2)',\
	org.eclipse.equinox.metatype;version='[1.4.100,1.4.101)',\
	org.h2;version='[1.4.192,1.4.193)',\
	org.hibernate.common.hibernate-commons-annotations;version='[5.0.1,5.0.2)',\
	org.hibernate.core;version='[5.0.9,5.0.10)',\
	org.hibernate.entitymanager;version='[5.0.9,5.0.10)',\
	org.hibernate.osgi;version='[5.0.9,5.0.10)',\
	org.jboss.jandex;version='[2.0.0,2.0.1)',\
	org.jboss.logging.jboss-logging;version='[3.3.0,3.3.1)',\
	org.osgi.service.metatype;version='[1.3.0,1.3.1)',\
	osgi.enroute.examples.jdbc.addressbook.dao.provider;version=snapshot,\
	osgi.enroute.examples.jdbc.addressbook.dao.test;version=snapshot,\
	osgi.enroute.hamcrest.wrapper;version='[1.3.0,1.3.1)',\
	osgi.enroute.junit.wrapper;version='[4.12.0,4.12.1)',\
	osgi.enroute.logger.simple.provider;version='[1.1.1,1.1.2)',\
	tx-control-provider-jpa-local;version='[0.0.2,0.0.3)',\
	tx-control-service-local;version='[0.0.2,0.0.3)',\
	tx-control-service-xa;version='[0.0.2,0.0.3)'

-runpath: org.apache.geronimo.specs.geronimo-jta_1.1_spec

-runsystempackages: \
	javax.xml.stream;version=1.0,\
	javax.xml.stream.events;version=1.0,\
	javax.xml.stream.util;version=1.0,\
	javax.transaction;version=1.1,\
	javax.transaction.xa;version=1.1


-augment.org_hibernate_osgi= \
	org.hibernate.osgi;\
		capability:="org.osgi.service;objectClass=javax.persistence.spi.PersistenceProvider;effective:=active"

